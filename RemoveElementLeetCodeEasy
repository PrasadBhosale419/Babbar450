https://leetcode.com/problems/remove-element/
class Solution {
public:
    int removeElement(vector<int>& nums, int val) {
        vector<int>resArr;
        for(int i=0;i<nums.size();i++){
            if(nums[i]!=val){
            resArr.push_back(nums[i]);}
            else{
                continue;
            }
        }
        nums=resArr;
        return nums.size();
    }
};
